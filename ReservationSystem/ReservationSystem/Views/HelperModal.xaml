<Window x:Class="ReservationSystem.Views.HelperModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReservationSystem.Views"
        x:Name="helperModal"
        mc:Ignorable="d"
        Title="HelperModal" Height="450" Width="800">
    <Grid>
        <Image Source="{Binding ElementName=helperModal, Path=ImagePath}" Height="300" Width="500"></Image>
    </Grid>
</Window>
